<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "Z://hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="hcql.Employee" table="Employee">
<id name="emp_id">
<generator class="assigned"></generator>
</id>
<property name="emp_name"></property>
<property name="yr_experience"></property>
<property name="salary"></property>
</class>
</hibernate-mapping>